{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gql/src",
  "monorepo": true,
  "root": "apps/api-gql",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api-gql/tsconfig.app.json"
  },
  "projects": {
    "api-gql": {
      "type": "application",
      "root": "apps/api-gql",
      "entryFile": "main",
      "sourceRoot": "apps/api-gql/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gql/tsconfig.app.json"
      }
    },
    "api-rest": {
      "type": "application",
      "root": "apps/api-rest",
      "entryFile": "main",
      "sourceRoot": "apps/api-rest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-rest/tsconfig.app.json"
      }
    },
    "computed": {
      "type": "library",
      "root": "libs/computed",
      "entryFile": "index",
      "sourceRoot": "libs/computed/src",
      "compilerOptions": {
        "tsConfigPath": "libs/computed/tsconfig.lib.json"
      }
    },
    "internal": {
      "type": "library",
      "root": "libs/internal",
      "entryFile": "index",
      "sourceRoot": "libs/internal/src",
      "compilerOptions": {
        "tsConfigPath": "libs/internal/tsconfig.lib.json"
      }
    },
    "external": {
      "type": "library",
      "root": "libs/external",
      "entryFile": "index",
      "sourceRoot": "libs/external/src",
      "compilerOptions": {
        "tsConfigPath": "libs/external/tsconfig.lib.json"
      }
    }
  }
}
